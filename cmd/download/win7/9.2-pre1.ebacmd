Function encrypt(str)
	Dim int2, int3, int4, tmp, tmp2, tmp3
	Rnd(-1)
	Randomize 5
	int2 = Int((Len(encKey)) * Rnd + 1)
	tmp = CAsc(str)
	tmp2 = CAsc(encKey)
	For forVar = 0 To UBound(tmp) - 1
		int3 = forVar + int2
