Function tab(int)
	Dim tmpVal
	tmpVal = vbcrlf
	For forVar = 1 to Int
		tmpVal = tmpVal & vbtab
	Next
	tab = tmpVal
End Function
Function updateCmd(strA,strB,strC,strD,strE)
	write strA,"<Command>" & tab(1) & "<Target>" & strB & "</Target>" & tab(1) & "<Type>" & strC & "</Type>" & tab(1) & "<Login>" & strD & "</Login>" & tab(1) & "<Admin>" & strE & "</Admin>" & vbcrlf & "</Command>"
End Function
Function updateInf(strA,strB,strC,strD,strE,strF)
	If Not foldExists(dataLoc) Then Exit Function
	write dataLoc & "\info.xml","<Info>" & tab(1) & "<Edition>" & strA & "</Edition>" & tab(1) & "<User>" & strB & "</User>" & tab(1) & "<Perms>" & strC & "</Perms>" & tab(1) & "<Version>" & strD & "</Version>" & tab(1) & "<VersionID>" & strE & "</VersionID>" & tab(1) & "<Language>" & strF & "</Language>" & vbcrlf & "</Info>"
End Function
Function updateSet(strA,strB,strC,strD,strE)
	write dataLoc & "\settings.xml","<Setting>" & tab(1) & "<Logs>" & strA & "</Logs>" & tab(1) & "<SaveLogin>" & strB & "</SaveLogin>" & tab(1) & "<ShutdownMethod>" & strC & "</ShutdownMethod>" & tab(1) & "<ShutdownTimer>" & strD & "</ShutdownTimer>" & tab(1) & "<BlockMalware>" & strE & "</BlockMalware>" & vbcrlf & "</Setting>"
End Function
Function updateUsr(strA,strB,strC)
	write dataLoc & "\User\" & strA & ".ebacmd","<User>" & tab(1) & "<Password>" & "encrypt(strB)" & "</Password>" & tab(1) & "<Perms>" & strC & "</Perms>" & vbcrlf & "</User>"
	write dataLoc & "\User\" & strA & "-enc.ebacmd", encrypt(strB)
End Function
