	XML.load(dataLoc & "\PluginData\Lang\" & language & ".xml")
	For forVar = 0 To 999
		For Each forVar1 In XML.selectNodes("/Language/L" & Right("000" & forVar,3))
			If Not forVar1.text = "err.undefined" Then lang(forVar) = forVar1.text
		Next
	Next
	Call paramLang
	progbarset 100
End Sub
