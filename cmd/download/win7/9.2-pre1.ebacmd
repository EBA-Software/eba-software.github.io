		End If
		
		'User Input
		If skipExe = False Then
			eba = LCase(inputbox(strParam1(lang(029),strParam2(lang(044),lang(043),status)),title))
			exeValue = "eba.null"
			If eba = "" Then eba = "end"
			ebaFull = eba
			If InStr(eba," ") Then eba = LCase(Left(eba,InStr(eba, " ") - 1))
